{
  "name": "oao",
  "version": "0.3.2",
  "description": "A Yarn-based, opinionated monorepo management tool",
  "bin": {
    "oao": "lib/index.js"
  },
  "scripts": {
    "build": "yarn run lint && yarn run compile && yarn run xxl",
    "lint": "eslint src",
    "compile": "rm -rf lib && babel src -d lib"
  },
  "repository": "guigrpa/oao",
  "keywords": [
    "monorepo",
    "lerna",
    "mono-repo",
    "yarn",
    "publish",
    "git"
  ],
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guigrpa/mono-repo/issues"
  },
  "homepage": "https://github.com/guigrpa/mono-repo#readme",
  "dependencies": {
    "commander": "2.9.0",
    "globby": "6.1.0",
    "inquirer": "3.0.1",
    "babel-polyfill": "6.22.0",
    "semver": "5.3.0",
    "shelljs": "0.7.6",
    "split": "1.0.0",
    "storyboard": "^2.0.0",
    "timm": "^1.2.3"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-flowtype": "2.30.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "xxl": "^1.0.1"
  }
}
